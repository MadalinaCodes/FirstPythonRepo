Meet IT Factory09:57
def sum_2_nr(nr1, nr2):
    suma = nr1 + nr2
    return suma


def is_par(nr):
    if nr % 2 == 0:
        return True
    else:
        return False


def total_chars(
        nume,
        prenume,
        nume_mijlociu
):
    total = len(nume) + len(prenume) + len(nume_mijlociu)
    return total


def aria_dreptunghiului(lungimea, latimea):
    aria = lungimea * latimea
    return aria

def aria_cercului(raza):
    pi = 3.14159265359
    aria = pi * raza * raza
    return aria
